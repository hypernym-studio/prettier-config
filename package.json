{
  "name": "@hypernym/prettier-config",
  "version": "1.0.1",
  "author": "Hypernym Studio",
  "description": "Hypernym's internal config for Prettier.",
  "license": "MIT",
  "repository": "hypernym-studio/prettier-config",
  "homepage": "https://github.com/hypernym-studio/prettier-config",
  "funding": "https://github.com/sponsors/ivodolenc",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "prettier",
    "prettier-config",
    "presets",
    "config",
    "esm"
  ],
  "scripts": {
    "build": "rolli",
    "bump": "bummp",
    "format": "prettier --config src/index.js --write .",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "prettier": ">=3.0.0"
  },
  "devDependencies": {
    "bummp": "^0.2.0",
    "prettier": "^3.0.3",
    "rolli": "^0.6.0"
  },
  "engines": {
    "node": ">=v18.0.0"
  }
}
