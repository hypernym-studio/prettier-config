{
  "name": "@hypernym/prettier-config",
  "version": "3.2.1",
  "author": "Hypernym Studio",
  "description": "Hypernym's internal config for Prettier.",
  "license": "MIT",
  "repository": "hypernym-studio/prettier-config",
  "homepage": "https://github.com/hypernym-studio/prettier-config",
  "funding": "https://github.com/sponsors/ivodolenc",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./svelte": {
      "types": "./dist/types/svelte/index.d.mts",
      "import": "./dist/svelte/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "prettier",
    "prettier-config",
    "presets",
    "config",
    "esm"
  ],
  "scripts": {
    "build": "hyperbundler",
    "format": "prettier --config src/index.js --write .",
    "prepublishOnly": "pnpm build"
  },
  "sideEffects": false,
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "peerDependencies": {
    "prettier": ">=3.0.0",
    "prettier-plugin-svelte": ">=3.2.3"
  },
  "peerDependenciesMeta": {
    "prettier-plugin-svelte": {
      "optional": true
    }
  },
  "devDependencies": {
    "@hypernym/bundler": "^0.14.0",
    "@hypernym/tsconfig": "^2.4.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7"
  }
}
